<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <AssemblyName>Constellix.Dns</AssemblyName>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>0.8.0</Version>
    <LangVersion>8.0</LangVersion>
    <NullableContextOptions>enable</NullableContextOptions>

    <!-- Package Details-->
    <Description>A modern .NETStandard Library for the Constellix DNS API</Description>
    <PackageTags>Constellix netstandard</PackageTags>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Carbon.Json" Version="1.9.0" />
  </ItemGroup>

</Project>